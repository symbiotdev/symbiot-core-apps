{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@symbiot-core-apps/account": ["libs/account/src/index.ts"],
      "@symbiot-core-apps/api": ["libs/api/src/index.ts"],
      "@symbiot-core-apps/app": ["libs/app/src/index.ts"],
      "@symbiot-core-apps/app-preferences": [
        "libs/app-preferences/src/index.ts"
      ],
      "@symbiot-core-apps/auth": ["libs/auth/src/index.ts"],
      "@symbiot-core-apps/brand": ["libs/brand/src/index.ts"],
      "@symbiot-core-apps/brand-client": ["libs/brand-client/src/index.ts"],
      "@symbiot-core-apps/brand-employee": ["libs/brand-employee/src/index.ts"],
      "@symbiot-core-apps/brand-location": ["libs/brand-location/src/index.ts"],
      "@symbiot-core-apps/brand-service": ["libs/brand-service/src/index.ts"],
      "@symbiot-core-apps/faq": ["libs/faq/src/index.ts"],
      "@symbiot-core-apps/form-controller": [
        "libs/form-controller/src/index.ts"
      ],
      "@symbiot-core-apps/i18n": ["libs/i18n/src/index.ts"],
      "@symbiot-core-apps/location": ["libs/location/src/index.ts"],
      "@symbiot-core-apps/notification": ["libs/notification/src/index.ts"],
      "@symbiot-core-apps/onboarding": ["libs/onboarding/src/index.ts"],
      "@symbiot-core-apps/shared": ["libs/shared/src/index.ts"],
      "@symbiot-core-apps/state": ["libs/state/src/index.ts"],
      "@symbiot-core-apps/storage": ["libs/storage/src/index.ts"],
      "@symbiot-core-apps/survey": ["libs/survey/src/index.ts"],
      "@symbiot-core-apps/theme": ["libs/theme/src/index.ts"],
      "@symbiot-core-apps/ui": ["libs/ui/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
